<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mask.pressboxlaundry">

    <uses-permission android:name="android.permission.INTERNET" />
    >
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".LoginPage"
            android:screenOrientation="portrait" />
        <activity
            android:name=".RegisterPage"
            android:label="Register for Laundry Services"
            android:screenOrientation="portrait" />
        <activity
            android:name=".FirstPage"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name=".ProfilePage"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name=".ClothPage"
            android:label="Cloth Count"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AboutPage"
            android:label="Help"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name=".SettingsPage"
            android:label="Settings"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name=".EditPage"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Prepared_clothes"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Unfinished_cloth"
            android:screenOrientation="portrait" />
        <activity android:name="com.mico.collegelaundry.RegisterPage2"></activity>
    </application>

</manifest>